    "        if Answer == 'get up' or Answer == 'I get up':\n",
    "            print('You begin to walk towards the light.')\n",
    "            break\n",
    "        elif Answer == 'wait':\n",
    "            print('you wait, no one comes and you die...')\n",
    "            break\n",
    "        else:\n",
    "            print('try again...')\n",
    "    #second question\n",
    "def secondquestion():\n",
    "    while True:\n",
